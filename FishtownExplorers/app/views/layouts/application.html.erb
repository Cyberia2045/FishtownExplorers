<!DOCTYPE html>
<html>
<head>
  <title>FishtownExplorers</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>

</head>

<body>

<nav class="navbar navbar-inverse navbar-fixed-top">
<div class="container-fluid">
  <!-- Brand and toggle get grouped for better mobile display -->
  <div class="navbar-header">
    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
      <span class="sr-only">Toggle navigation</span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
      <span class="icon-bar"></span>
    </button>
      <% if current_user %>
        <%= link_to "Fishtown Explorers", user_profile_path(current_user.id), class: "fishtown_brand" %>
      <% else %>  
        <%= link_to "Fishtown Explorers", root_path %>
      <% end %>
  </div>

  <!-- Collect the nav links, forms, and other content for toggling -->
  <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
    <ul class="nav navbar-nav">
    </ul>
    <ul class="nav navbar-nav navbar-right">
      <% if current_user %>
      <li class="active"><%= link_to "Logout", destroy_user_session_path, method: :delete %><span class="sr-only">(current)</span></a></li>
      <% else %>
      <li class="active"><%= link_to "Login", user_session_path %><span class="sr-only">(current)</span></a></li>
      <% end %>
    </ul>
  </div><!-- /.navbar-collapse -->
</div><!-- /.container-fluid -->
</nav>

     <p class="notice"><%= notice %></p>
     <p class="alert"><%= alert %></p>

  <%= yield %>

  <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCsbGBG4DuYkmK8v-fpIi5vjdDMY1XjqBM&callback=initMap">
  </script>

</body>
</html>
